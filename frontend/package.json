{
  "name": "hospital-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc && vite build",
    "build:win": "tsc && set NODE_OPTIONS=--max-old-space-size=8192 && vite build",
    "lint:check": "biome check src",
    "lint:fix": "biome lint src && biome format src --write",
    "lint:apply": "biome check src --apply",
    "typecheck": "tsc --noEmit",
    "test": "vitest --run",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "openapi:generate-types": "tsx scripts/generate_api_types.ts",
    "gen:icons": "node scripts/generate-icon-types.mjs",
    "gen:types": "node scripts/generate-types.js"
